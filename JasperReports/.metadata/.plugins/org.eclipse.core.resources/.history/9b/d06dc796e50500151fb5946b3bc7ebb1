	<parameter name="prmStartDate" class="java.sql.Date">
		<parameterDescription><![CDATA[Start Date:]]></parameterDescription>
	</parameter>
	<parameter name="prmEndDate" class="java.sql.Date">
		<parameterDescription><![CDATA[End Date:]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select  Project_Type,  
concat( substr(min(date_format(week, '%M')),1,3), min(date_format(week, ' %Y')))  as month_name, 
date_format(week, '%Y%m') as month_no, sum(total_booked_Hours) as total_booked_Hours ,  
sum(total_booked_core_hours) as total_booked_core_hours,  
sum(total_booked_extra_hours) as total_booked_extra_hours,
datekey
from itr_db.v_timesheet_booked_time
where  week >=  $P{prmStartDate} 
and week <= $P{prmEndDate} 
 group by Project_Type, month_no
order by month_no]]>
	</queryString>
	<field name="total_booked_Hours" class="java.lang.Double">
